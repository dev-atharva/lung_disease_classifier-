      },
      "source": [
        "\n",
        "def mapping(prediction):\n",
        "  if np.argmax(prediction)==0:\n",
        "    print(\"Covid\")\n",
        "  if np.argmax(prediction)==1:\n",
        "    print(\"Healthy\")\n",
        "  if np.argmax(prediction)==2:\n",
        "    print(\"Pneumonia\") \n",
        "\n",
        "result=mapping(prediction)\n",
        "#print(result)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "Pneumonia\n"
          ],
          "name": "stdout"
        }
      ]
